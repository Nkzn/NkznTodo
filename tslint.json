{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": { "severity": "none" },
    "interface-name": { "severity": "none" },
    "object-literal-sort-keys": { "severity": "none" },
    "member-access": [true, "no-public"]
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
